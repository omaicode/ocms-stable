{
    "name": "ocms/socialite",
    "description": "Laravel wrapper around OAuth 1 & OAuth 2 libraries.",
    "homepage": "https://omaicode.com",
    "require": {
        "php": "^7.2|^8.0",
        "ext-json": "*",
        "guzzlehttp/guzzle": "^6.0|^7.0",
        "illuminate/contracts": "^6.0|^7.0|^8.0|^9.0|^10.0|^11.0",
        "illuminate/http": "^6.0|^7.0|^8.0|^9.0|^10.0|^11.0",
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0|^10.0|^11.0",
        "league/oauth1-client": "^1.10.1"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^4.0|^5.0|^6.0|^7.0|^8.0|^9.0",
        "phpstan/phpstan": "^1.10",
        "phpunit/phpunit": "^8.0|^9.3|^10.4"
    },
    "autoload": {
        "psr-4": {
            "OCMS\\Socialite\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "OCMS\\Socialite\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "5.x-dev"
        },
        "laravel": {
            "providers": [
                "OCMS\\Socialite\\SocialiteServiceProvider"
            ],
            "aliases": {
                "Socialite": "OCMS\\Socialite\\Facades\\Socialite"
            }
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
